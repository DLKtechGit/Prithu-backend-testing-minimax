# Environment Configuration Template
# Copy this file to .env and update the values

# API Configuration
EXPO_PUBLIC_API_URL=http://192.168.1.66:5000
EXPO_PUBLIC_WS_URL=http://192.168.1.66:5000

# Feature Flags
EXPO_PUBLIC_ENABLE_NOTIFICATIONS=true
EXPO_PUBLIC_ENABLE_WEBSOCKET=true
EXPO_PUBLIC_ENABLE_ANALYTICS=false

# Notification Configuration
EXPO_PUBLIC_FCM_PROJECT_ID=your-fcm-project-id

# Image Assets
EXPO_PUBLIC_COMPANY_LOGO_URL=https://your-cdn.com/logo.png
EXPO_PUBLIC_DEFAULT_IMAGE_URL=https://your-cdn.com/default-image.jpg

# Debug Settings
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_LOG_LEVEL=info

# External Services
EXPO_PUBLIC_SENTRY_DSN=
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=